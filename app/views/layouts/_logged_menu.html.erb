<li class="dropdown">
  <a href="#" class="dropdown-toggle" data-toggle="dropdown">
    <%= t 'app.register.other' %>
    <span class="caret"></span>
  </a>
  <ul class="dropdown-menu" role="menu">
  <% if can? :read, Role %>
    <li><%= link_to t_model_name(:role, :other) , roles_path %></li>
  <% end %>
  <% if can? :read, User %>
    <li><%= link_to t_model_name(:user, :other) , users_path %></li>
  <% end %>
  <% if can? :read, Sender %>
    <li><%= link_to t_model_name(:sender, :other) , senders_path %></li>
  <% end %>
  </ul>
</li>